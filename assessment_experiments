    "# NORMALISE THE NUMERICAL ROWS\n",
    "\n",
    "from sklearn.preprocessing import MinMaxScaler\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "scaler = MinMaxScaler()\n",
    "# numerical_cols = columns_by_type[\"number\"] <--- missing salary hence hardcoded for now\n",
    "numerical_cols = [\"YearsCode\",\"YearsCodePro\",\"WorkExp\"]\n",
    "# TODO - replace NaNs with something useful\n",
    "\n",
    "\n",
    "# Assign 50 to maintain consistency with other numerical values.\n",
    "# This choice makes sense for analysis because it doesn’t distort the data while preserving its intent.\n",
    "\n",
    "survey_df[numerical_cols] = survey_df[numerical_cols].replace('More than 50 years', 50).replace('Less than 1 year', 0)\n",
    "\n",
    "\n",
    "\n",
    "survey_df[numerical_cols] = scaler.fit_transform(survey_df[numerical_cols])\n",
    "\n",
    "\n",
    "survey_sample_df = survey_df[numerical_cols].sample(n=10, random_state=42)\n",
    "print(survey_sample_df)\n",
