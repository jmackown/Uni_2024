    "import pandas as pd\n",
    "\n",
    "survey_data_path = \"/kaggle/input/stackoverflow-survey-2024/survey_results_public.csv\"        \n",
    "survey_schema_path = \"/kaggle/input/stackoverflow-survey-2024/survey_results_schema.csv\"\n",
    "\n",
    "df = pd.read_csv(survey_data_path)\n",
    "\n",
    "# cols = [\"Age\",\"Employment\",\"EdLevel\",\"LearnCode\",\"YearsCode\",\"YearsCodePro\",\"DevType\",\"OrgSize\",\"PurchaseInfluence\",\"Country\",\"TimeSearching\",\"TimeAnswering\",\"CompTotal\",]\n",
    "\n",
    "df = df.drop_duplicates()\n",
    "df[\"CompTotal\"] = pd.to_numeric(df[\"CompTotal\"], errors='coerce')\n",
    "df[\"CompTotal\"] = df[\"CompTotal\"].fillna(0)\n",
    "df['CompTotal'] = df['CompTotal']/1000\n",
    "df = df[df['CompTotal'].notna()]\n",
    "df = df[df['CompTotal'] > 15]\n",
    "df['percentile'] = df['CompTotal'].rank(pct=True) * 100\n",
    "\n",
    "df = df[df['percentile'] >= 2]\n",
    "df = df[df['percentile'] <= 80]\n",
    "\n",
    "df['p90'] = df['CompTotal'].quantile(0.9)\n",
    "\n",
    "print(df.shape[0])\n",
    "\n",
    "cols = [\"Age\", \"DevType\", \"CompTotal\", \"percentile\", \"p90\"]\n",
    "df = df[cols]\n",
    "df = df.sort_values(by='CompTotal', ascending=True)\n"
   ]
